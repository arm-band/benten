{
    "name": "benten",
    "version": "4.1.2",
    "description": "自分用のスライド制作用テンプレートです。",
    "main": "gulpfile.js",
    "scripts": {
        "om": "mkdirp dist dist/content dist/css dist/css/print dist/css/theme dist/js dist/lib dist/lib/js dist/lib/css dist/lib/font dist/lib/font/league-gothic dist/lib/font/source-sans-pro dist/fonts",
        "sarasvati": "npm-run-all -p benten su namo byakuja gyo ugaya jaya gyarabei shinda mani hinden um svaha2",
        "svaha": "gulp init",
        "benten": "copyfiles -f \"./node_modules/font-awesome/fonts/*\" \"./dist/fonts/\"",
        "su": "copyfiles -f \"./node_modules/reveal.js/css/*.css\" \"./dist/css/\"",
        "namo": "copyfiles -f \"./node_modules/reveal.js/css/theme/*.css\" \"./dist/css/theme/\"",
        "byakuja": "copyfiles -f \"./node_modules/reveal.js/css/print/*.css\" \"./dist/css/print/\"",
        "gyo": "copyfiles -f \"./node_modules/reveal.js/js/*\" \"./dist/js/\"",
        "ugaya": "copyfiles -f \"./node_modules/reveal.js/lib/js/*\" \"./dist/lib/js/\"",
        "jaya": "copyfiles -f \"./node_modules/reveal.js/lib/css/*\" \"./dist/lib/css/\"",
        "gyarabei": "copyfiles -f \"./node_modules/reveal.js/lib/font/league-gothic/*\" \"./dist/lib/font/league-gothic/\"",
        "shinda": "copyfiles -f \"./node_modules/reveal.js/lib/font/source-sans-pro/*\" \"./dist/lib/font/source-sans-pro/\"",
        "mani": "copyfiles -f \"./node_modules/reveal.js/plugin/highlight/*\" \"./dist/plugin/highlight\"",
        "hinden": "copyfiles -f \"./node_modules/reveal.js/plugin/markdown/*\" \"./dist/plugin/markdown\"",
        "um": "copyfiles -f \"./node_modules/reveal.js/plugin/notes/*\" \"./dist/plugin/notes\"",
        "svaha2": "copyfiles -f \"./node_modules/reveal.js/plugin/zoom-js/*\" \"./dist/plugin/zoom-js\"",
        "biwa": "rimraf ./dist/ ./package-lock.json",
        "start": "npm-run-all -s om sarasvati svaha",
        "test": "gulp server"
    },
    "author": "Arm Band",
    "license": "ISC",
    "devDependencies": {
        "brand-colors": "^2.0.1",
        "browser-sync": "^2.24.4",
        "copyfiles": "^2.0.0",
        "font-awesome": "^4.7.0",
        "gulp": "^4.0.0",
        "gulp-autoprefixer": "^5.0.0",
        "gulp-data": "^1.3.1",
        "gulp-ejs": "^3.1.3",
        "gulp-imagemin": "^4.1.0",
        "gulp-json-to-sass": "^0.0.4",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.0",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^4.0.1",
        "gulp-watch": "^5.0.1",
        "mkdirp": "^0.5.1",
        "npm-run-all": "^4.1.3",
        "require-dir": "^1.1.0",
        "reveal.js": "^3.6.0",
        "rimraf": "^2.6.2",
        "yaml": "^1.0.0-rc.8"
    }
}
