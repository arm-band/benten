{
    "name": "benten",
    "version": "4.2.1",
    "description": "自分用のスライド制作用テンプレートです。",
    "main": "gulpfile.js",
    "scripts": {
        "om": "mkdirp dist dist/content dist/css dist/css/print dist/css/theme dist/js dist/lib dist/lib/js dist/lib/css dist/lib/font dist/lib/font/league-gothic dist/lib/font/source-sans-pro dist/fonts dist/webfonts",
        "sarasvati": "npm-run-all -p benten su namo byakuja gyo ugaya jaya gyarabei shinda mani hinden um svaha2",
        "svaha": "gulp",
        "benten": "cpx \"./node_modules/@fortawesome/fontawesome-free/webfonts/**/*\" \"./dist/webfonts/\"",
        "su": "cpx \"./node_modules/reveal.js/css/*.css\" \"./dist/css/\"",
        "namo": "cpx \"./node_modules/reveal.js/css/theme/*.css\" \"./dist/css/theme/\"",
        "byakuja": "cpx \"./node_modules/reveal.js/css/print/*.css\" \"./dist/css/print/\"",
        "gyo": "cpx \"./node_modules/reveal.js/js/*\" \"./dist/js/\"",
        "ugaya": "cpx \"./node_modules/reveal.js/lib/js/*\" \"./dist/lib/js/\"",
        "jaya": "cpx \"./node_modules/reveal.js/lib/css/*\" \"./dist/lib/css/\"",
        "gyarabei": "cpx \"./node_modules/reveal.js/lib/font/league-gothic/*\" \"./dist/lib/font/league-gothic/\"",
        "shinda": "cpx \"./node_modules/reveal.js/lib/font/source-sans-pro/*\" \"./dist/lib/font/source-sans-pro/\"",
        "mani": "cpx \"./node_modules/reveal.js/plugin/highlight/*\" \"./dist/plugin/highlight\"",
        "hinden": "cpx \"./node_modules/reveal.js/plugin/markdown/*\" \"./dist/plugin/markdown\"",
        "um": "cpx \"./node_modules/reveal.js/plugin/notes/*\" \"./dist/plugin/notes\"",
        "svaha2": "cpx \"./node_modules/reveal.js/plugin/zoom-js/*\" \"./dist/plugin/zoom-js\"",
        "biwa": "rimraf ./dist/ ./package-lock.json",
        "start": "npm-run-all -s om sarasvati svaha",
        "restart": "gulp server"
    },
    "author": "Arm Band",
    "license": "ISC",
    "devDependencies": {
        "brand-colors": "^2.1.1",
        "browser-sync": "^2.26.7",
        "cpx": "^1.5.0",
        "@fortawesome/fontawesome-free": "^5.13.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-data": "^1.3.1",
        "gulp-ejs": "^5.1.0",
        "gulp-imagemin": "^7.1.0",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-pngquant": "^8.0.0",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-svgo": "^7.1.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-watch": "^5.0.1",
        "mkdirp": "^1.0.4",
        "npm-run-all": "^4.1.5",
        "reveal.js": "^3.9.2",
        "rimraf": "^3.0.2",
        "yaml": "^1.8.3"
    }
}
