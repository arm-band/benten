  "main": "gulpfile.js",
  "scripts": {
    "proinit:mkdir": "mkdirp dist dist/content dist/favicon dist/css dist/css/print dist/css/theme dist/js dist/lib dist/lib/js dist/lib/css dist/lib/font dist/lib/font/league-gothic dist/lib/font/source-sans-pro dist/fonts",
    "proinit:fa_fonts_copy": "copyfiles -f \"./node_modules/font-awesome/fonts/*\" \"./dist/fonts/\"",
    "proinit:reveal_css_copy1": "copyfiles -f \"./node_modules/reveal.js/css/*.css\" \"./dist/css/\"",
    "proinit:reveal_css_copy2": "copyfiles -f \"./node_modules/reveal.js/css/theme/*.css\" \"./dist/css/theme/\"",
    "proinit:reveal_css_copy3": "copyfiles -f \"./node_modules/reveal.js/css/print/*.css\" \"./dist/css/print/\"",
    "proinit:reveal_js_copy1": "copyfiles -f \"./node_modules/reveal.js/js/*\" \"./dist/js/\"",
    "proinit:reveal_lib_copy1": "copyfiles -f \"./node_modules/reveal.js/lib/js/*\" \"./dist/lib/js/\"",
    "proinit:reveal_lib_copy2": "copyfiles -f \"./node_modules/reveal.js/lib/css/*\" \"./dist/lib/css/\"",
    "proinit:reveal_lib_copy3": "copyfiles -f \"./node_modules/reveal.js/lib/font/league-gothic/*\" \"./dist/lib/font/league-gothic/\"",
    "proinit:reveal_lib_copy4": "copyfiles -f \"./node_modules/reveal.js/lib/font/source-sans-pro/*\" \"./dist/lib/font/source-sans-pro/\"",
    "proinit:reveal_plugin_copy1": "copyfiles -f \"./node_modules/reveal.js/plugin/highlight/*\" \"./dist/plugin/highlight\"",
    "proinit:reveal_plugin_copy2": "copyfiles -f \"./node_modules/reveal.js/plugin/markdown/*\" \"./dist/plugin/markdown\"",
    "proinit:reveal_plugin_copy3": "copyfiles -f \"./node_modules/reveal.js/plugin/notes/*\" \"./dist/plugin/notes\"",
    "proinit:reveal_plugin_copy4": "copyfiles -f \"./node_modules/reveal.js/plugin/zoom-js/*\" \"./dist/plugin/zoom-js\"",
    "proinit": "npm-run-all -s proinit:mkdir proinit:md_copy proinit:fa_fonts_copy proinit:reveal_css_copy1 proinit:reveal_css_copy2 proinit:reveal_css_copy3 proinit:reveal_js_copy1 proinit:reveal_lib_copy1 proinit:reveal_lib_copy2 proinit:reveal_lib_copy3 proinit:reveal_plugin_copy1 proinit:reveal_plugin_copy2 proinit:reveal_plugin_copy3 proinit:reveal_plugin_copy4",
    "om": "npm run proinit:mkdir",
    "sarasvati": "npm run proinit:fa_fonts_copy",
    "svaha": "npm run proinit:reveal_css_copy1",
    "namo": "npm run proinit:reveal_css_copy2",
    "byakuja": "npm run proinit:reveal_css_copy3",
    "gyo": "npm run proinit:reveal_js_copy1",
    "ugaya": "npm run proinit:reveal_lib_copy1",
    "jaya": "npm run proinit:reveal_lib_copy2",
    "gyarabei": "npm run proinit:reveal_lib_copy3",
    "shinda": "npm run proinit:reveal_lib_copy4",
    "mani": "npm run proinit:reveal_plugin_copy1",
    "hinden": "npm run proinit:reveal_plugin_copy2",
    "um": "npm run proinit:reveal_plugin_copy3",
    "svaha2": "npm run proinit:reveal_plugin_copy4",
    "benten": "npm-run-all -s om sarasvati svaha namo byakuja gyo ugaya jaya gyarabei shinda mani hinden um svaha2"
  },
