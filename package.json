    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "gulp-autoprefixer": "^5.0.0",
    "jquery": "^3.3.1",
    "jquery.easing": "^1.4.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.3",
    "rename-files-cli": "^1.0.1",
    "gulp-data": "^1.3.1",
    "gulp-json-to-sass": "^0.0.4",
    "brand-colors": "^2.0.1",
